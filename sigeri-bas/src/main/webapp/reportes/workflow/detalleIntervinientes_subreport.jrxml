	<parameter name="LIQUIDACION" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select distinct(sol.ci_ruc),li.num_tramite_rp, li.fecha_creacion ,
case sol.es_persona when true then
coalesce(sol.nombres,'')||' '||coalesce(sol.apellidos,'')
else sol.razon_social end AS interviniente,
case fn.es_persona when true then
coalesce(fn.nombres,'')||' '||coalesce(fn.apellidos,'')
else fn.razon_social end AS funcionario,pa.papel
from flow.regp_intervinientes it
left join flow.regp_liquidacion_detalles ld on (ld.id = it.liquidacion)
left join flow.regp_liquidacion li on (li.id = ld.liquidacion)
left join app.cat_ente sol on (sol.id = it.ente)
left join app.acl_user u on (u.id = li.user_creacion)
left join app.cat_ente fn on (fn.id = u.ente)
left join app.reg_papel pa on (pa.id = it.papel)
where ld.liquidacion = $P{LIQUIDACION}
group by 1,2,3,4,5,6
order by li.fecha_creacion]]>
	</queryString>
