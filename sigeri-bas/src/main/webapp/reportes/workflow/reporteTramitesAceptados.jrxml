	<queryString>
		<![CDATA[select li.id, sum(li.total_pagar) as total
from flow.regp_liquidacion li
left join app.acl_user acl on (acl.id = li.user_creacion)
left join app.acl_user_has_rol ur on (ur.acl_user = acl.id)
left join app.acl_rol rol on (rol.id = ur.acl_rol)
where li.estado_liquidacion in (1,2)
and rol.nombre = 'revisor'
and fecha_creacion between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
group by li.id
order by li.fecha_creacion]]>
	</queryString>
	<field name="id" class="java.lang.Long"/>
	<field name="total" class="java.math.BigDecimal"/>
	<variable name="total_1" class="java.math.BigDecimal" calculation="Sum">
