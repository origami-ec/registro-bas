	<parameter name="LIQUIDACION" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select ac.nombre as acto,
de.cuantia, de.avaluo, de.valor_unitario,de.descuento, de.valor_total,
li.desc_limit_cobro, li.total_pagar, (li.total_pagar + li.desc_limit_cobro) as subtotal
from flow.regp_liquidacion_detalles de
left join app.reg_acto ac on (ac.id = de.acto)
left join flow.regp_liquidacion li on (li.id = de.liquidacion)
where de.liquidacion = $P{LIQUIDACION}]]>
	</queryString>
