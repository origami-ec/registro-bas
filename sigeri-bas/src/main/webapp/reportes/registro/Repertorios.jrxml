	<queryString>
		<![CDATA[select cast(m.fecha as date) as fecha_repertorio, to_char(m.fecha, 'dd/MM/yyyy') as date_repertorio,
app.mes_dia_cadena (1, to_char(m.fecha, 'D')) as fecha_dia, to_char(m.fecha, 'DD') as fecha_dia_num,
app.mes_dia_cadena (2, to_char(m.fecha, 'MM')) as fecha_mes, to_char(m.fecha, 'yyyy') as fecha_anio,
count(distinct(m.repertorio)) as cantidad, min(m.repertorio) as minimo, max(m.repertorio) as maximo,
cast(now() as date) - cast(m.fecha as date) as dias_diferencia
from secuencias.secuencia_repertorio m
where m.fecha between to_date($P{FECHA_DESDE}, 'dd/MM/yyyy') and to_date($P{FECHA_HASTA}, 'dd/MM/yyyy')
group by fecha_repertorio, date_repertorio, fecha_dia, fecha_dia_num, fecha_mes, fecha_anio
order by 1]]>
	</queryString>
