	<parameter name="MOVIMIENTO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT trim(i.ced_ruc) || ' - ' || trim(i.nombre) || ' - ' ||trim(pa.papel) as interviniente,
i.ced_ruc, i.nombre, pa.papel
FROM app.reg_movimiento_cliente c
LEFT OUTER JOIN app.reg_ente_interviniente i ON (c.ente_interv=i.id)
LEFT OUTER JOIN app.reg_papel pa ON pa.id = c.papel
WHERE c.movimiento = $P{MOVIMIENTO}]]>
	</queryString>
