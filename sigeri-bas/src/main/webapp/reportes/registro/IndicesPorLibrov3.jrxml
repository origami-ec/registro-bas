	<parameter name="INSC_HASTA" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select mo.num_inscripcion, mo.observacion,
(select trim(f.descripcion_bien) from app.reg_ficha f
left join app.reg_movimiento_ficha mf on mf.ficha = f.id
where mf.movimiento = mo.id limit 1) as descripcion_bien,
(select trim(ei.nombre) from app.reg_ente_interviniente ei
left join app.reg_movimiento_cliente mc on ei.id = mc.ente_interv
where mc.movimiento = mo.id limit 1) as nombre
from app.reg_movimiento mo
where mo.num_inscripcion is not null and mo.libro = $P{LIBRO}
and to_char(mo.fecha_inscripcion, 'yyyy') = $P{ANIO}
and mo.num_inscripcion between $P{INSC_DESDE} and $P{INSC_HASTA}
order by 4 ]]>
	</queryString>
