	<queryString>
		<![CDATA[select mov.id as movimiento_id, mov.num_inscripcion, mov.fecha_inscripcion, mov.folio_inicio,
mov.num_tomo, mov.folio_inicio, mov.folio_fin, mov.num_repertorio, mov.fecha_oto, mov.escrit_juic_prov_resolucion, mov.fecha_resolucion, mov.observacion,
acto.nombre as nombre_acto, libro.nombre as nombre_libro, ente.nombre as ente_judicial,
canton.nombre as nombre_canton, mov.avaluo_municipal_cadena, mov.cuantia_cadena,
mov.avaluo_municipal, mov.cuantia
from app.reg_movimiento_ficha as mov_ficha
left join app.reg_movimiento as mov on (mov.id = mov_ficha.movimiento)
left join app.reg_acto as acto on (acto.id = mov.acto)
left join app.reg_libro as libro on (libro.id = mov.libro)
left join app.reg_ente_judiciales as ente on (ente.id = mov.ente_judicial)
left join app.reg_domicilio as canton on (canton.id = mov.domicilio)
where mov_ficha.ficha = $P{id_ficha}
order by mov.fecha_inscripcion asc, mov.num_repertorio, mov.num_inscripcion]]>
	</queryString>
