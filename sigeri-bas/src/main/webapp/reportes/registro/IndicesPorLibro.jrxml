	<subDataset name="dataset1" uuid="92b32890-ca25-451b-805e-259077c0050c">
		<parameter name="MOVIMIENTO" class="java.lang.Long"/>
		<queryString>
			<![CDATA[select en.nombre as interviniente, pa.papel
from app.reg_movimiento_cliente mc
left join app.reg_ente_interviniente en on (en.id = mc.ente_interv)
left join app.reg_papel pa on pa.id = mc.papel
where mc.movimiento = $P{MOVIMIENTO}
order by pa.papel desc, en.nombre asc]]>
		</queryString>
		<field name="interviniente" class="java.lang.String"/>
		<field name="papel" class="java.lang.String"/>
	</subDataset>
	<parameter name="LIBRO" class="java.lang.Long"/>
	<parameter name="USER_NAME" class="java.lang.String"/>
	<parameter name="LIBRO_NAME" class="java.lang.String"/>
	<parameter name="HEADER_URL" class="java.lang.String"/>
	<parameter name="NRO_TOMO" class="java.lang.String"/>
	<parameter name="ANIO" class="java.lang.String"/>
	<queryString>
		<![CDATA[select mo.id as movimiento, mo.num_inscripcion, mo.fecha_inscripcion, mo.num_repertorio,
mo.folio_inicio, mo.folio_fin,
(select f.descripcion_bien from app.reg_ficha f left join app.reg_movimiento_ficha mf on mf.ficha = f.id where mf.movimiento = mo.id limit 1) as descripcion_bien
from app.reg_movimiento mo
left join app.reg_movimiento_ficha mf on (mf.movimiento = mo.id)
where mo.estado = 'AC' and mo.libro = $P{LIBRO}
and to_char(mo.fecha_inscripcion, 'yyyy') = $P{ANIO}
and mo.num_tomo = $P{NRO_TOMO}
order by mo.num_inscripcion]]>
	</queryString>
	<field name="movimiento" class="java.lang.Long"/>
	<field name="num_inscripcion" class="java.lang.Integer"/>
	<field name="fecha_inscripcion" class="java.sql.Timestamp"/>
	<field name="num_repertorio" class="java.lang.Integer"/>
	<field name="folio_inicio" class="java.lang.Integer"/>
	<field name="folio_fin" class="java.lang.Integer"/>
	<field name="descripcion_bien" class="java.lang.String"/>
