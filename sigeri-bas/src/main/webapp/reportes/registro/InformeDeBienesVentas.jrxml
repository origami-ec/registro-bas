	<queryString>
		<![CDATA[select l.nombre as libroname, mov.num_inscripcion as numins,
mov.fecha_inscripcion as fecins, mov.num_repertorio as numrep,
a.nombre as actoname, mov.folio_inicio as folioini, pa.papel,
mov.registro, mov.observacion, mov.num_tramite,
(select f.clave_catastral from app.reg_ficha f
left join app.reg_movimiento_ficha mf on mf.ficha = f.id
where mf.movimiento = mov.id limit 1) as clave_catastral,
(select f.num_ficha from app.reg_ficha f
left join app.reg_movimiento_ficha mf on mf.ficha = f.id
where mf.movimiento = mov.id limit 1) as num_ficha,
(select f.descripcion_bien from app.reg_ficha f
left join app.reg_movimiento_ficha mf on mf.ficha = f.id
where mf.movimiento = mov.id limit 1) as descripcion_bien
from app.reg_ente_interviniente renti
left join app.reg_movimiento_cliente as mc on (mc.ente_interv=renti.id)
left join app.reg_movimiento as mov on (mov.id=mc.movimiento and mov.num_inscripcion is not null
and mov.estado in ('AC', 'IN'))
left join app.reg_libro as l on (l.id=mov.libro)
left join app.reg_acto as a on (a.id=mov.acto)
left join app.reg_papel as pa on (pa.id=mc.papel)
where renti.nombre ilike $P{BUSQUEDA} || '%'
and l.id = 11 and a.anexo_uno_reg_propiedad and pa.propietario is false
order by mov.fecha_inscripcion, mov.num_inscripcion]]>
	</queryString>
