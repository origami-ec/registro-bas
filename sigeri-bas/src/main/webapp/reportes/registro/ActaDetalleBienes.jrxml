	<parameter name="P_MOVIMIENTO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select f.clave_catastral as codigo_predial, f.num_ficha, f.linderos,
f.descripcion_bien, pa.descripcion as parroquia, f.tipo_ficha
from app.reg_movimiento_ficha h
left join app.reg_movimiento r on(h.movimiento=r.id)
left join app.reg_ficha f on( h.ficha=f.id)
left join app.cat_parroquia pa on pa.id = f.parroquia
where r.id=$P{P_MOVIMIENTO};]]>
	</queryString>
