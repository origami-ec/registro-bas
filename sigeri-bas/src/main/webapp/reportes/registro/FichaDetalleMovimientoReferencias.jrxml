	<parameter name="id_movimiento" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select mov.num_inscripcion, mov.fecha_inscripcion, mov.num_repertorio,
mov.folio_inicio, mov.folio_fin, libro.nombre, ac.nombre as acto
from app.reg_movimiento_referencia as ref
left join app.reg_movimiento as mov on (mov.id = ref.movimiento_referencia)
left join app.reg_libro as libro on (libro.id = mov.libro)
left join app.reg_acto as ac on (ac.id = mov.acto)
where ref.movimiento = $P{id_movimiento}
order by mov.fecha_inscripcion, mov.num_inscripcion, mov.num_repertorio]]>
	</queryString>
