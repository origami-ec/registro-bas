	<queryString>
		<![CDATA[select li.id, li.num_tramite_rp, li.fecha_creacion, li.codigo_comprobante,
li.numero_comprobante, us.usuario, li.certificado, li.inscripcion,
li.estado_liquidacion, li.inscripcion, li.reingreso, sol.ci_ruc, li.fecha_ingreso,  li.titulo_credito,
(li.total_pagar + li.descuento_valor + li.desc_limit_cobro) as sub_total,
(li.descuento_valor + li.desc_limit_cobro) as descuento,
li.total_pagar,
case sol.es_persona when true then
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'') else sol.razon_social end as solicitante,ins.usuario as certificador
from flow.regp_liquidacion li
left join app.acl_user us on us.id = li.user_creacion
left join app.cat_ente sol on sol.id = li.solicitante
left join app.acl_user ins on ins.id = li.inscriptor
where
(CASE WHEN $P{USUARIO} > 0 THEN us.id = $P{USUARIO} ELSE us.id > 0 END) and
li.fecha_creacion between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone and li.estado_liquidacion = 1
order by li.num_tramite_rp]]>
	</queryString>
