	<queryString>
		<![CDATA[select li.num_tramite_rp, li.total_pagar, li.codigo_comprobante, us.usuario, li.inscripcion,
li.fecha_ingreso, ins.usuario as certificador, sol.ci_ruc,
case sol.es_persona when true then
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'') else sol.razon_social end as solicitante
from flow.regp_liquidacion li
left join app.acl_user us on us.id = li.user_creacion
left join app.acl_user ins on ins.id = li.inscriptor
left join app.cat_ente sol on sol.id = li.solicitante
left join flow.regp_liquidacion_detalles det on det.liquidacion = li.id
left join app.reg_acto ac on ac.id = det.acto
where li.es_juridico and li.version_descuento = 3 and
(CASE WHEN $P{USUARIO} > 0 THEN us.id = $P{USUARIO} ELSE us.id > 0 END)
and li.fecha_ingreso between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
group by li.num_tramite_rp, li.total_pagar, li.codigo_comprobante, us.usuario, li.inscripcion,
li.fecha_ingreso, ins.usuario,  sol.ci_ruc, sol.es_persona, sol.apellidos, sol.nombres, sol.razon_social
order by li.num_tramite_rp]]>
	</queryString>
