	<queryString>
		<![CDATA[select p.pro_fechaing as fecha_ingreso,
p.pro_numerocomp::character varying as numero_comprobante,
p.pro_nombrefact as solicitante,
p.pro_codigo as num_proforma,
substring(p.pro_usuario,7) as usuario,
p.pro_valreg as subtotal, p.pro_valmat as dcto, (p.pro_valreg-p.pro_valmat) as total,
p.pro_cedulafact as ci_ruc,
p.pro_motivo
from historico.proforma p
where p.pro_status = 2
and p.pro_fechaing between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by 1,2]]>
