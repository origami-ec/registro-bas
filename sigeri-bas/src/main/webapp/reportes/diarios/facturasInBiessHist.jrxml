	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<parameter name="HASTA_STRING" class="java.lang.String"/>
	<parameter name="USUARIO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select us.usuario, liq.numero_comprobante, liq.num_tramite_rp, liq.fecha_ingreso,
liq.sub_total, (liq.descuento_valor + liq.desc_limit_cobro) as descuento,
liq.total_pagar, sol.ci_ruc, liq.codigo_comprobante,
case sol.es_persona when TRUE THEN
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'')
else sol.razon_social end AS solicitante
from flow.regp_liquidacion liq
left join app.cat_ente sol on (sol.id = liq.solicitante)
left join app.acl_user us on (us.id = liq.user_ingreso)
where liq.estado_liquidacion = 2 and
(CASE WHEN $P{USUARIO}>0 THEN liq.user_ingreso=$P{USUARIO} ELSE liq.user_ingreso>0 END)
and liq.fecha_ingreso between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by liq.numero_comprobante, us.usuario, liq.num_tramite_rp]]>
	</queryString>
