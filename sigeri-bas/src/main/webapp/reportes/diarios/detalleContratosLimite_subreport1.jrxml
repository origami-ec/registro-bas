	<parameter name="LIQUIDACION" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select ac.nombre as acto, de.cuantia, de.avaluo, de.valor_unitario, li.codigo_comprobante,
de.descuento, de.valor_total, li.num_tramite_rp, li.fecha_ingreso,
li.numero_comprobante, sol.ci_ruc,
case sol.es_persona when true then
coalesce(sol.nombres,'')||' '||coalesce(sol.apellidos,'')
else sol.razon_social end AS solicitante,
li.desc_limit_cobro, li.total_pagar, (li.total_pagar + li.desc_limit_cobro) as subtotal
from flow.regp_liquidacion_detalles de
left join app.reg_acto ac on (ac.id = de.acto)
left join flow.regp_liquidacion li on (li.id = de.liquidacion)
left join app.cat_ente sol on (sol.id = li.solicitante)
where de.liquidacion = $P{LIQUIDACION} order by de.fecha_ingreso]]>
	</queryString>
