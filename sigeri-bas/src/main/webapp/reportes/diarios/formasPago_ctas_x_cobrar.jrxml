	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="USUARIO" class="java.lang.Long"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[select liq.fecha_ingreso, liq.numero_comprobante, liq.num_tramite_rp, liq.codigo_comprobante,
case sol.es_persona when TRUE then coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'')
else sol.razon_social end AS solicitante, sol.ci_ruc, liq.total_pagar
from flow.regp_liquidacion liq
left join app.cat_ente sol on (sol.id = liq.solicitante)
where liq.estado_pago = 3 and liq.estado_liquidacion = 2 and liq.reingreso = false and
(CASE WHEN $P{USUARIO}>0 THEN liq.user_ingreso=$P{USUARIO} ELSE liq.user_ingreso>0 END)
and liq.fecha_ingreso between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
 order by liq.fecha_ingreso]]>
	</queryString>
