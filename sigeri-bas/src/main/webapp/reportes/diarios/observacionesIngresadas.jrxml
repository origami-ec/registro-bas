	<queryString>
		<![CDATA[select ob.numero, li.num_tramite_rp, ob.fecha_ingreso,
CASE sol.es_persona when TRUE THEN
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'')
else sol.razon_social end AS cliente, sol.ci_ruc as documento,
ob.observacion1, ob.observacion2, ob.estado, acl.usuario, li.id as liquidacion
from flow.regp_observaciones_ingreso ob
left join app.cat_ente sol on (sol.id = ob.ente)
left join app.acl_user acl on (acl.id = ob.user_ingreso)
left join app.acl_user_has_rol ur on ur.acl_user = acl.id
left join app.acl_rol rol on rol.id = ur.acl_rol
left join flow.regp_liquidacion li on li.id = ob.liquidacion
where
(CASE WHEN $P{ID_USER}>0 THEN ob.user_ingreso=$P{ID_USER} ELSE ob.user_ingreso>0 END)
and ob.fecha_ingreso between $P{DESDE} and $P{HASTA}
and rol.nombre = 'revisor' order by ob.fecha_ingreso]]>
	</queryString>
