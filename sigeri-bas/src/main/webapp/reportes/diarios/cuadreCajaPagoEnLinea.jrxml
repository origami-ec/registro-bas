	<queryString>
		<![CDATA[select
initcap(coalesce(ente_usuario.nombres,'')||' '||coalesce(ente_usuario.apellidos,'')) recaudador,
liq.numero_comprobante,
us.usuario,
liq.num_tramite_rp,
liq.fecha_ingreso,
liq.sub_total,
(liq.descuento_valor + liq.desc_limit_cobro) as descuento,
liq.total_pagar,
bene.ci_ruc,
liq.codigo_comprobante,
case bene.es_persona when TRUE THEN
coalesce(bene.apellidos,'')||' '||coalesce(bene.nombres,'')
else bene.razon_social end AS solicitante
from flow.regp_liquidacion liq
left join app.cat_ente bene on (bene.id = liq.beneficiario)
left join app.acl_user us on (us.id = liq.user_ingreso)
left join app.cat_ente ente_usuario on ente_usuario.id = us.ente
left join financiero.ren_nota_credito nc on nc.factura = liq.id
where liq.reingreso = false and liq.estado_liquidacion = 2 and
us.usuario = 'rpventanilla'
and liq.fecha_ingreso between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
union all
select
initcap(coalesce(ente_usuario.nombres,'')||' '||coalesce(ente_usuario.apellidos,'')) recaudador,
fa.numero_comprobante,
us.usuario,
fa.num_tramite as num_tramite_rp,
fa.fecha as fecha_ingreso,
fa.sub_total,
rp.descuento,
fa.total_pagar,
sol.ci_ruc,
fa.codigo_comprobante,
CASE sol.es_persona WHEN TRUE THEN coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'') ELSE sol.razon_social END AS solicitante
from financiero.ren_factura fa
left join financiero.ren_cajero ca on ca.id = fa.caja
left join app.acl_user us on us.id = ca.usuario
left join app.cat_ente ente_usuario on ente_usuario.id = us.ente
left join app.cat_ente sol on sol.id = fa.solicitante
left join financiero.ren_pago rp on  rp.id = fa.pago
where us.usuario = 'rpventanilla'
and fa.fecha between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without  time zone AND
to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND fa.estado = true
order by 1, 2, 3]]>
	</queryString>
