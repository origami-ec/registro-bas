	<queryString>
		<![CDATA[select li.num_tramite_rp, ht.fecha_ingreso, ht.fecha_entrega, ob.fec_cre, li.repertorio,
ob.user_cre, li.id as liquidacion
from flow.observaciones ob
left join flow.historico_tramites ht on ht.id = ob.id_tramite
left join flow.regp_liquidacion li on li.tramite = ht.id
where ob.tarea = 'Marginar' and ob.estado
and ob.fec_cre between $P{DESDE} and $P{HASTA} order by ob.fec_cre]]>
	</queryString>
