	<queryString>
		<![CDATA[select fa.codigo_comprobante, fa.clave_acceso, fa.numero_autorizacion,
fa.estado_ws, us.usuario as cajero, fa.fecha, sol.ci_ruc as documento,
fa.total_pagar, CASE sol.es_persona WHEN TRUE THEN
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'')
ELSE sol.razon_social END AS solicitante
from financiero.ren_factura fa
left join financiero.ren_cajero ca on ca.id = fa.caja
left join app.acl_user us on us.id = ca.usuario
left join app.cat_ente sol on sol.id = fa.solicitante
where (CASE WHEN $P{USUARIO}>0 THEN us.id=$P{USUARIO} ELSE us.id>0 END)
and fa.fecha between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without  time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND fa.estado = true
order by fa.fecha]]>
	</queryString>
