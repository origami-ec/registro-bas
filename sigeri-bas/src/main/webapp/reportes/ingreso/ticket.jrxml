	<queryString>
		<![CDATA[select ht.fecha_creacion, ht.fecha_edicion, ht.num_tramite_rp, en.ci_ruc as documento,
case en.es_persona when true then
coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'')
else en.razon_social end AS solicitante,
ht.inscripcion, ht.certificado, us.usuario as inscriptor, ht.fecha_ingreso
from flow.regp_liquidacion ht
left join app.cat_ente en on en.id = ht.beneficiario
left join app.acl_user us on us.id = ht.inscriptor
where ht.id = $P{ID}]]>
