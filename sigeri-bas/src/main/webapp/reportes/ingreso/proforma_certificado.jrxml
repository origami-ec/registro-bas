	<queryString>
		<![CDATA[select liq.fecha_creacion, ht.num_tramite, sol.ci_ruc, sol.telefono1, sol.direccion, sol.correo1, ben.ci_ruc as ced_bene, ben.telefono1 as telef_bene, ben.direccion as direccion_bene, ben.correo1 as correo_bene,
case sol.es_persona when true then coalesce(sol.nombres,'')||' '||coalesce(sol.apellidos,'')
else sol.razon_social end AS solicitante,
case ben.es_persona when true then coalesce(ben.nombres,'')||' '||coalesce(ben.apellidos,'')
else ben.razon_social end AS beneficiario,
liq.sub_total, liq.descuento_valor, liq.desc_limit_cobro, liq.total_pagar, liq.descuento_porc,
liq.gastos_generales,
liq.adicional,
coalesce(en.titulo_prof,'') ||' '||
coalesce(en.nombres,'') ||' '|| coalesce(en.apellidos,'') as user_name,
liq.oficio_memo_referencia from flow.regp_liquidacion as liq
left join flow.historico_tramites ht on (ht.id = liq.tramite)
left join app.cat_ente sol on (sol.id = liq.solicitante)
left join app.cat_ente ben on (ben.id = liq.beneficiario)
left join app.acl_user us on (us.id = liq.user_creacion)
left join app.cat_ente en on (en.id = us.ente)
where liq.id = $P{ID_LIQUIDACION}]]>
	</queryString>
