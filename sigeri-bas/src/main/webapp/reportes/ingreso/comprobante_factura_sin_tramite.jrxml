	<queryString>
		<![CDATA[select
fact.pago as pago_id,
ht.num_tramite,
--ht.fecha_ingreso,
fact.fecha_emision fecha_ingreso,
ht.fecha_entrega,
pago.valor as total_pagar ,
pago.valor as valor_actos,
liq.desc_limit_cobro,
liq.sub_total,
fact.clave_acceso,
fact.codigo_comprobante,
fact.numero_autorizacion,
en.ci_ruc as doc_beneficiario,
case en.es_persona when true then
coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'')
else en.razon_social end AS beneficiario,
fact.fecha_autorizacion,
en.direccion, en.telefono1,
us.usuario,
pago.descuento as descuento_valor,
financiero.tipo_pago(pago.id) as tipo_pago


from financiero.ren_factura fact
left join flow.regp_liquidacion liq on fact.liquidacion = liq.id
left join flow.historico_tramites ht on ht.id = liq.tramite
left join financiero.ren_pago pago on fact.pago = pago.id
left join app.cat_ente en on en.id = fact.solicitante
left join  financiero.ren_cajero cajero on fact.caja = cajero.id
left join app.acl_user us on us.id = cajero.usuario
where fact.id = $P{ID_LIQUIDACION}]]>
	</queryString>
