	<parameter name="LIQUIDACION" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select de.id as detalle, ac.nombre as acto, de.valor_unitario, de.descuento, de.valor_total, de.descuento_promedio, case en.es_persona when true then
coalesce(en.apellidos,'')||' '||coalesce(en.nombres,'')
else en.razon_social end AS solicitante,
exo.concepto, exo.valor, pa.papel
from flow.regp_intervinientes rv
left join flow.regp_liquidacion_detalles de on rv.liquidacion = de.id
left join flow.regp_exoneracion exo on exo.id = rv.exoneracion
left join app.reg_acto ac on ac.id = de.acto
left join app.cat_ente en on en.id = rv.ente
left join app.reg_papel pa on pa.id = rv.papel
where de.liquidacion = $P{LIQUIDACION} and rv.exoneracion is not null
order by de.fecha_ingreso]]>
	</queryString>
