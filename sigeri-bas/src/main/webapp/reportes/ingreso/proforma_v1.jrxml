	<queryString>
		<![CDATA[select liq.fecha_creacion, ht.num_tramite, sol.ci_ruc, sol.telefono1, sol.direccion, sol.correo1, ben.ci_ruc as ced_bene, ben.telefono1 as telef_bene, ben.direccion as direccion_bene, ben.correo1 as correo_bene, liq.ingreso_direccion, liq.ingreso_num_repertorio, liq.ingreso_fecha_repertorio,
liq.ingreso_fecha_inscripcion, liq.ingreso_linderos, liq.ingreso_historiado, liq.ingreso_ventas,
liq.ingreso_num_historiado, liq.ingreso_num_ventas,
case sol.es_persona when true then coalesce(sol.nombres,'')||' '||coalesce(sol.apellidos,'')
else sol.razon_social end AS solicitante,
case ben.es_persona when true then coalesce(ben.nombres,'')||' '||coalesce(ben.apellidos,'')
else ben.razon_social end AS beneficiario,
liq.sub_total, liq.descuento_valor, liq.desc_limit_cobro, liq.total_pagar, liq.descuento_porc,
liq.adicional, liq.ficha, liq.num_inscripcion, liq.anio_inscripcion,
liq.gastos_generales, it.valor as uso_documento, liq.inf_adicional_prof,
coalesce(en.titulo_prof,'') ||' '||
coalesce(en.nombres,'') ||' '|| coalesce(en.apellidos,'') as user_name, us.usuario as elaborado,
liq.oficio_memo_referencia, liq.num_formulario, liq.estado_ws, liq.mensaje_ws,
liq.certificado_no_poseer_bien, liq.tramite_corporativo, liq.ride_enviado, pa.descripcion as parroquia
from flow.regp_liquidacion as liq
left join flow.historico_tramites ht on (ht.id = liq.tramite)
left join app.cat_ente sol on (sol.id = liq.solicitante)
left join app.cat_ente ben on (ben.id = liq.beneficiario)
left join app.acl_user us on (us.id = liq.user_creacion)
left join app.cat_ente en on (en.id = us.ente)
left join app.ctlg_item it on (it.id = liq.uso_documento)
left join app.cat_parroquia pa on pa.id = liq.parroquia
where liq.id = $P{ID_LIQUIDACION}]]>
	</queryString>
